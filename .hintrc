{
  "extends": [
    "development"
  ],
  "hints": {
    "apple-touch-icons": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "text-align: -webkit-match-parent"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 115",
    "not chrome <= 114",
    "not edge <= 114",
    "not firefox <= 114",
    "not safari <= 16.5",
    "not samsung <= 21"
  ]
}